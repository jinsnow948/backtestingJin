{
  "version": 1,
  "disable_existing_loggers": false,
  "root": {
    "handlers": ["console"],
    "level": "DEBUG"
  },
  "loggers": {
    "main": {
      "handlers": ["console","file"],
      "level": "DEBUG",
      "propagate": true
    }
  },
  "formatters": {
      "basic": {
        "format": "[%(asctime)-15s] (%(filename)s:%(lineno)d) %(module)s:%(levelname)s - %(message)s",
        "datefmt": "%Y-%m-%d %H:%M:%S"
      }
  },
  "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "DEBUG",
        "formatter": "basic"
      },
      "file": {
        "class": "logging.FileHandler",
        "level": "INFO",
        "formatter": "basic",
        "filename": "../log/info.log"
      }
  }
}